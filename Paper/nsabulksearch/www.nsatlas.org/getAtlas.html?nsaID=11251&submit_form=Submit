<script>
function setSearch() {
	// reload the page, change search= to searchType
	rads = document.formSelect.elements['formSelect'];
	for (i in rads) {
      if (rads[i].checked == true)
			window.location.search = "?search=" + rads[i].value;
	}
}
function setCutout() {
	// reload the page, change cutoutType= to cutoutType
	rads = document.formCutout.elements['formCutout'];
	for (i in rads) {
		if (rads[i].checked == true)
			 window.location.search = jQuery.query.set("cutoutType", rads[i].value);
	}
}
</script>

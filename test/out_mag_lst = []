        out_mag_lst = []   
        catalog2 = open(cat2,'r')
        out_coord = []
        for line in catalog:
            line = line.split()
            if (line[0]!='#'):
                ra = float(line[2])
                dec = float(line[3])
                mag=float(line[10])#*10**(9)
                out_coord.append([ra,dec])
                out_mag_lst.append(mag)
        out_coord = np.array(out_coord)
        out_ra_lst = out_coord[::,0]
        out_dec_lst = out_coord[::,1]
